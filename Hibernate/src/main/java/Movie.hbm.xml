<hibernate-mapping>
	<class name = "src/main/java.Movie" table="MOVIES">
		<id name="id" column="ID">
			<generator class="native"/>
		</id>
		<property name="title" column="TITLE"/>
		<property name="director" column="DIRECTOR"/>
		<property name="synopsis" column="SYNOPSIS"/>
	</class>
</hibernate-mapping>